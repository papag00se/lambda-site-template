{
    "name": "lambda.site.template",
    "version": "0.0.1",
    "description": "lambda.site.template",
    "author": "https://github.com/papag00se",
    "type": "module",
    "license": "UNLICENSE",
    "dependencies": {
        "@lit-labs/ssr-dom-shim": "^1.4.0",
        "mime": "^4.1.0",
        "urlpattern-polyfill": "^10.0.0"
    },
    "scripts": {
        "dev": "NODE_ENV=preprod nodemon -r dotenv/config --trace-warnings backend/local.js",
        "prod": "NODE_ENV=production nodemon --trace-warnings backend/local.js",
        "build:frontend": "rm -rf frontend/dist && rollup -c ./deployment/rollup.frontend.config.js && cp frontend/$APP_ENV${APP_ENV:+.}env.js frontend/dist/env.js",
        "build:backend": "rollup -c ./deployment/rollup.backend.config.js",
        "sync:android": "(export APP_ENV=android; npm run build:frontend && npx cap sync android)",
        "sync:ios": "(export APP_ENV=ios npm run build:frontend && npx cap sync ios)"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.537.0",
        "@capacitor/android": "^7.4.4",
        "@capacitor/cli": "^7.4.4",
        "@capacitor/core": "^7.4.4",
        "@capacitor/ios": "^7.4.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-inject": "^5.0.5",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@types/aws-lambda": "^8.10.92",
        "@types/cors": "^2.8.13",
        "@types/estree": "^1.0.0",
        "@types/mime": "^3.0.4",
        "@types/node": "^18",
        "dotenv": "^16.4.5",
        "nodemon": "^3.1.0",
        "prettier": "^3",
        "rollup": "^4.12.0"
    }
}
